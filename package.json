{
  "name": "proto-to-open-rpc",
  "version": "1.0.0",
  "description": "A utility to convert Protocol Buffer RPC definitions to OpenRPC format",
  "main": "dist/index.js",
  "bin": {
    "proto-to-open-rpc": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "ts-node src/cli.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
